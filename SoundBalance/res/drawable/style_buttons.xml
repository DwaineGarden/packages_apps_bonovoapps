            <gradient android:startColor="#ff878787" android:endColor="#ff7a7a7a" android:angle="270.0" />
            <stroke android:width="2.0dip" android:color="#ff878787" />
            <corners android:radius="10.0dip" />
            <padding android:left="10.0dip" android:top="10.0dip" android:right="10.0dip" android:bottom="10.0dip" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <gradient android:startColor="#ff0033ff" android:endColor="#ff03738c" android:angle="90.0" />
            <stroke android:width="1.0dip" android:color="#ff03738c" />
